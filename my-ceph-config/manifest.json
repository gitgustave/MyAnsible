{
    "__version": "45D-R1",
    "name": "ceph-ansible-45d",
    "title": "Ceph Ansible",
    "prerelease": false,
    "version": "5.4.6",
    "buildVersion": "1",
    "author": "Brett Kelly <bkelly@45drives.com>",
    "url": "https://github.com/45Drives/ceph-ansible",
    "category": "utils",
    "priority": "optional",
    "licence": "GPL-3.0+",
    "architecture": {
        "deb": "all",
        "el": "noarch"
    },
    "description": {
        "long": "Ansible playbooks to deploy Ceph with 45Drives-specific changes.",
        "short": "Ansible playbooks to deploy Ceph with 45Drives-specific changes."
    },
    "defaults": {
        "urgency": "medium"
    },
    "dependencies": {
        "deb": [
            "ansible",
            "ansible-core",
            "python3-netaddr",
            "python3",
            "samba-ansible (>= 1.1.3)"
        ],
        "el": [
            "ansible = 5.6.0",
            "ansible-core = 2.12.2",
            "python3",
            "python38-netaddr",
            "python38-pyyaml",
            "python38-six",
            "samba-ansible >= 1.1.3"
        ]
    },
    "releases": [
        {
            "image": "ubuntu-focal-builder",
            "codeName": "focal",
            "type": "deb"
        },
        {
            "image": "rocky-el8-builder",
            "codeName": "el8",
            "type": "el"
        }
    ],
    "changelog": {
        "urgency": "medium",
        "version": "5.4.6",
        "buildVersion": "1",
        "ignore": [],
        "date": null,
        "packager": "Brett Kelly <bkelly@45drives.com>",
        "changes": []
    }
